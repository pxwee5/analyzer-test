    "lodash-es": "^4.17.11",
    "promise-polyfill": "^8.1.0",
    "smoothscroll-polyfill": "^0.4.4",
    "svgstore": "^3.0.0-2",
    "svgxuse": "^1.2.6",
    "url-polyfill": "^1.1.5",
    "vue": "^2.6.10",
    "vue-disqus": "^3.0.5",
    "vue-tel-input": "^2.3.0",
    "vue-template-compiler": "^2.6.10",
    "vuelidate": "^0.7.4",
    "vuex": "^3.0.1",
    "whatwg-fetch": "^3.0.0"
