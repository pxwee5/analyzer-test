{
  "name": "raken-website",
  "version": "2.0.0",
  "description": "Raken App's Public Facing Website",
  "private": true,
  "scripts": {
    "start": "rm -rf ./dist && webpack-dev-server --inline --hot --mode development",
    "build": "rm -rf ./dist && webpack --mode production",
    "analyze": "webpack-bundle-analyzer --port 8888 dist/stats.json",
    "test": "npm run build && npm run cypress:run",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:run_on_blue": "cypress run --env baseUrl=https://www-blue.rakenapp.com",
    "build:svg": "node build/svgSpriteGenerator.js"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.1.0",
    "@babel/runtime": "^7.4.5",
    "@vue/eslint-config-prettier": "^4.0.1",
    "autoprefixer": "^9.3.1",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "css-loader": "^2.1.1",
    "cypress": "3.2.0",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.2.2",
    "file-loader": "^2.0.0",
    "mini-css-extract-plugin": "^0.4.4",
    "node-sass": "^4.10.0",
    "node-sass-json-importer": "^4.1.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.16.4",
    "pretty-quick": "^1.8.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "svg-sprite-loader": "^4.1.3",
    "tailwindcss": "^1.0.1",
    "terser-webpack-plugin": "^1.2.3",
    "uglifyjs-webpack-plugin": "^2.1.1",
    "vue-loader": "^15.7.0",
    "vue-style-loader": "^4.1.2",
    "webpack": "^4.28.4",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.14",
    "webpack-manifest-plugin": "^2.0.4",
    "webpack-merge": "^4.1.4"
  },
  "dependencies": {
